<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>


<SplitPane dividerPositions="0.2" prefHeight="700.0" prefWidth="1094.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HandlersControllers.MainWindowController">
   <items>
      <Pane>
         <children>
            <Button fx:id="addStudent" layoutX="3.0" layoutY="442.0" mnemonicParsing="false" onAction="#addStudClick" prefHeight="25.0" prefWidth="124.0" text="Student hinzufügen" />
            <Button fx:id="editData" layoutX="1.0" layoutY="592.0" mnemonicParsing="false" onAction="#edit" prefHeight="25.0" prefWidth="126.0" text="Daten bearbeiten" />
            <Button fx:id="delStudent" layoutX="2.0" layoutY="542.0" mnemonicParsing="false" onAction="#delStudentOnClick" prefHeight="25.0" prefWidth="126.0" text="Student exmatrikulieren" />
            <Button fx:id="addCourse" layoutX="3.0" layoutY="495.0" mnemonicParsing="false" onAction="#addCourseOnClick" prefHeight="25.0" prefWidth="123.0" text="Kurs hinzufügenn" />
         </children>
      </Pane>
      <TableView fx:id="studList" prefHeight="698.0" prefWidth="316.0">
         <columns>
            <TableColumn prefWidth="177.0" resizable="false" text="Vorname" />
            <TableColumn prefWidth="168.0" resizable="false" text="Nachname" />
            <TableColumn prefWidth="100.0" resizable="false" text="Kurs" />
            <TableColumn prefWidth="125.0" resizable="false" text="Mat.-Nr," />
            <TableColumn prefWidth="191.0" resizable="false" text="Firma" />
            <TableColumn prefWidth="110.0" resizable="false" text="Java Kenntnisse" />
         </columns>
      </TableView>
   </items>
</SplitPane>
